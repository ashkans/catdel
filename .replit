entrypoint = "streamlit run catdel/main.py"
run = ["streamlit", "run", "catdel/main.py", "--server.headless", "true"]

modules = ["python-3.10"]

hidden = [".pythonlibs"]

[nix]
channel = "stable-23_05"


[deployment]
run = ["streamlit", "run", "--server.address", "0.0.0.0", "--server.headless", "true", "--server.enableCORS=false", "--server.enableWebsocketCompression=false", "--server.runOnSave=false", "catdel/main.py"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 8501
externalPort = 80
